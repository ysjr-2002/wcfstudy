<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="HzDataService.Apple" behaviorConfiguration="mybehavior" >
        <endpoint address="phone" binding="wsDualHttpBinding" contract="HzDataService.IPhone"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://192.168.1.173:8899/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <wsDualHttpBinding>
        <binding name="mydual">
          <security mode="None"></security>
        </binding>
      </wsDualHttpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mybehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>